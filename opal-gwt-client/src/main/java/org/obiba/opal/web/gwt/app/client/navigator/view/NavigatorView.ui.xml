<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui" 
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

  
  <o:WorkbenchLayout>
    <g:Label><ui:msg description="Datasources label">Datasources</ui:msg></g:Label>
    <g:Label></g:Label>
    <g:FlowPanel>
      <g:Button ui:field="createDatasourceButton" styleName="btn">
        <ui:msg description="Create Datasource label">Create Datasource</ui:msg>
      </g:Button>
      <g:Button ui:field="importDataButton" styleName="btn">
        <ui:msg description="Data Import label">Import Data</ui:msg>
      </g:Button>
      <g:Button ui:field="exportDataButton" styleName="btn">
        <ui:msg description="Data Export label">Export Data</ui:msg>
      </g:Button>
      <o:HelpImage page="Working+with+Datasources"></o:HelpImage>
    </g:FlowPanel>
    <g:SplitLayoutPanel>
      <g:center> 
        <g:ScrollPanel width="100%" ui:field="navigatorDisplayPanel" />
      </g:center>
      <g:west size='240'>
        <g:ScrollPanel ui:field="treePanel">
        </g:ScrollPanel>
      </g:west>
    </g:SplitLayoutPanel>  
  </o:WorkbenchLayout>
  
</ui:UiBinder>
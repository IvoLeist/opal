<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
    ui:generateLocales="default">

  <g:DialogBox ui:field="dialogBox" modal="false">
    <g:DockLayoutPanel styleName="main" ui:field="content" unit="EM" height="520px" width="480px">
      <g:center>
          <g:FlowPanel ui:field="panel">
          </g:FlowPanel>
      </g:center>
      <g:south size="4">
        <g:FlowPanel addStyleNames="footer">
          <g:Button styleName="btn right-aligned top-margin" ui:field="closeButton">
            <ui:msg description="Close label">Close</ui:msg>
          </g:Button>
        </g:FlowPanel>
      </g:south>
    </g:DockLayoutPanel>
  </g:DialogBox>
</ui:UiBinder>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui" 
  xmlns:g2="urn:import:com.google.gwt.user.cellview.client" 
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view"  
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

  <ui:style>
    .toolbar-right {
      margin-left: 5px;
    }
    .toolbar img:hover {
      cursor: pointer;
    }
  </ui:style>

  <g:FlowPanel>
    <o:SuggestListBox ui:field="suggestList" visible="false"/>
    <g:HorizontalPanel>
   	  <g:ListBox ui:field="tableList" multipleSelect="true"></g:ListBox>
   	  <g:VerticalPanel addStyleNames="{style.toolbar},{style.toolbar-right}">
        <g:Image ui:field="addImage" url="image/20/list-add.png"></g:Image>
        <g:Image ui:field="removeImage" url="image/20/list-remove.png"></g:Image>
   	  </g:VerticalPanel>	
    </g:HorizontalPanel>
  </g:FlowPanel>
</ui:UiBinder>
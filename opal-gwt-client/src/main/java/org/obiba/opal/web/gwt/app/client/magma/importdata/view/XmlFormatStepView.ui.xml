<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
    ui:generateLocales="default">

    <g:FlowPanel styleName="input">
      <g:Label addStyleNames="key">
        <ui:msg description="Data File label">Data File</ui:msg>
      </g:Label>
      <g:SimplePanel ui:field="selectXmlFilePanel"/>
      <g:Label styleName="help-block">
        <ui:msg description="Opal XML File Description label">Data in Opal XML format are packaged in an archive file
          (.zip)
        </ui:msg>
      </g:Label>
    </g:FlowPanel>

</ui:UiBinder>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:g2="urn:import:com.google.gwt.user.cellview.client" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" ui:generateLocales="default">

  <g:DockLayoutPanel unit="EM">
    <g:north size="4">
      <g:LayoutPanel addStyleNames="top">
        <g:layer top="0" height="2em">
          <g:FlowPanel  ui:field="topBar" addStyleNames="topbar">
            <g:Anchor ui:field="quit"><ui:msg description="Quit link text">Quit</ui:msg></g:Anchor>
          </g:FlowPanel>
        </g:layer>

        <g:layer bottom="0" height="2em">
          <g:MenuBar ui:field="menuBar" autoOpen="true">
            <g:MenuItem  ui:field="dashboardItem">
              <ui:msg description="Dashboard menu item">Dashboard</ui:msg>              
            </g:MenuItem>
            <g:MenuItem ui:field="datasourcesItem">
              <ui:msg description="Datasources label">Datasources</ui:msg>
              <g:MenuBar vertical="true">
                <g:MenuItem ui:field="exploreVariablesItem"><ui:msg description="Explore variables menu item">Explore variables...</ui:msg></g:MenuItem>
                <g:MenuItem styleName="gwt-MenuItemSeparator"><div class="menuSeparatorInner"></div></g:MenuItem> 
                <g:MenuItem>
                  <ui:msg description="New label">New</ui:msg>
                  <g:MenuBar vertical="true">
                    <g:MenuItem><ui:msg description="Datasource label">Datasource</ui:msg></g:MenuItem>
                    <g:MenuItem ui:field="newViewItem"><ui:msg description="View label">View</ui:msg></g:MenuItem>
                  </g:MenuBar>
                </g:MenuItem>
                <g:MenuItem styleName="gwt-MenuItemSeparator"><div class="menuSeparatorInner"></div></g:MenuItem>
                <g:MenuItem ui:field="importVariablesItem"><ui:msg description="Import variables menu item">Import Variables...</ui:msg></g:MenuItem>
                <g:MenuItem ui:field="dataImportItem"><ui:msg description="Import menu item">Import Data...</ui:msg></g:MenuItem>
                <g:MenuItem ui:field="dataExportItem"><ui:msg description="Export menu item">Export Data...</ui:msg></g:MenuItem>
              </g:MenuBar>
            </g:MenuItem>            
            <g:MenuItem ui:field="fileExplorer">
              <ui:msg description="Files menu item">Files</ui:msg>
            </g:MenuItem>
            <g:MenuItem ui:field="listJobsItem">
              <ui:msg description="Jobs menu item">Jobs</ui:msg>
            </g:MenuItem>           
            <!-- g:MenuItem>
              <ui:msg description="Administration menu item">Administration</ui:msg>
            </g:MenuItem-->
          </g:MenuBar>

        </g:layer>
      </g:LayoutPanel>
    </g:north>
    <g:center>
      <g:LayoutPanel ui:field="workbench" addStyleNames="main-content ui-corner-all">
        <g:layer>
          <g:Label><ui:msg description="Workbench label">The Workbench</ui:msg></g:Label>
        </g:layer>
      </g:LayoutPanel>
    </g:center>
  </g:DockLayoutPanel>
</ui:UiBinder>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
    xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.ui"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
    ui:generateLocales="default">

  <g:FlowPanel>
    <g:FlowPanel addStyleNames="main">
      <b:FluidRow>
        <b:Column size="6">
          <g:FlowPanel addStyleNames="extra-large-bottom-margin">
            <b:WellNavList>
              <b:NavHeader>
                <ui:msg description="Main Administration Data Access">Data Access</ui:msg>
              </b:NavHeader>
              <b:Divider/>
              <o:OpalNavLink ui:field="usersGroupsPlace" icon="COG">
                <ui:msg description="Main Administration Users and Applications title">Users and Applications</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Users and Applications help">Add, update, remove users and applications
                  </ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="profilesPlace" icon="COG">
                <ui:msg description="Main Administration Profiles title">Profiles</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Profiles help">Manage user and application profiles
                  </ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="unitsPlace" icon="COG">
                <ui:msg description="Main Administration Units title">Functional Units (deprecated)</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Units title">Manage identifiers and key stores in terms of functional units
                  </ui:msg>
                </g:Label>
              </g:FlowPanel>
            </b:WellNavList>

            <b:WellNavList>
              <b:NavHeader>
                <ui:msg description="Main Administration Data Management">Data Management</ui:msg>
              </b:NavHeader>
              <b:Divider/>
              <o:OpalNavLink ui:field="filesPlace" icon="COG">
                <ui:msg description="Main Administration Files title">Files</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Files help">Manage file system</ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="tasksPlace" icon="COG">
                <ui:msg description="Main Administration Tasks title">Tasks</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Tasks help">Monitor and schedule tasks</ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="reportsPlace" icon="COG">
                <ui:msg description="Main Administration Reports title">Reports</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Reports help">Configure and schedule reports</ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="identifiersPlace" icon="COG">
                <ui:msg description="Main Administration Identifiers title">Identifiers Mappings</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Identifiers title">Manage identifiers mappings
                  </ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="keystorePlace" icon="COG">
                <ui:msg description="Main Administration Keystore title">Keystore</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Keystore title">Manage certificates and private keys for encryption and decryption of data
                  </ui:msg>
                </g:Label>
              </g:FlowPanel>
            </b:WellNavList>

          </g:FlowPanel>
        </b:Column>

        <b:Column size="6">
          <g:FlowPanel addStyleNames="extra-large-bottom-margin">
            <b:WellNavList>
              <b:NavHeader>
                <ui:msg description="Main Administration Data Analysis">Data Analysis</ui:msg>
              </b:NavHeader>
              <b:Divider/>
              <o:OpalNavLink ui:field="rPlace" icon="COG">
                <ui:msg description="Main Administration R title">R</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration R help">Configure R server, grant access to R service
                  </ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="dataShieldPlace" icon="COG">
                <ui:msg description="Main Administration DataSHIELD title">DataSHIELD</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration DataSHIELD help">Configure DataSHIELD, grant access to
                    DataSHIELD service
                  </ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="searchPlace" icon="COG">
                <ui:msg description="Main Administration Index title">Search</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Index help">Configure Elastic Search server, schedule table indexing</ui:msg>
                </g:Label>
              </g:FlowPanel>
            </b:WellNavList>

            <b:WellNavList>
              <b:NavHeader>
                <ui:msg description="Main Administration System">System</ui:msg>
              </b:NavHeader>
              <b:Divider/>
              <o:OpalNavLink ui:field="serverPlace" icon="COG">
                <ui:msg description="Main Administration General Settings title">General Settings</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration General Settings help">Manage general server configuration
                  </ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="databasesPlace" icon="COG">
                <ui:msg description="Main Administration Databases title">Databases</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Databases help">
                    Manage identifiers database and data databases for storage, import and export
                  </ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>

              <o:OpalNavLink ui:field="pluginsPlace" icon="COG">
                <ui:msg description="Main Administration Plugins title">Plugins</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration Plugins help">Add, remove plugins for data visualization or
                    system monitoring
                  </ui:msg>
                </g:Label>
              </g:FlowPanel>
              <b:Divider/>
              <o:OpalNavLink ui:field="javaPlace" icon="COG">
                <ui:msg description="Main Administration JVM title">Java Virtual Machine</ui:msg>
              </o:OpalNavLink>
              <g:FlowPanel>
                <g:Label styleName="help-block">
                  <ui:msg description="Main Administration JVM help">Monitor Java virtual machine</ui:msg>
                </g:Label>
              </g:FlowPanel>
            </b:WellNavList>
          </g:FlowPanel>
        </b:Column>
      </b:FluidRow>
    </g:FlowPanel>
    <!--<g:HTMLPanel addStyleNames="administration">-->
    <!--<o:NavTabsPanel ui:field="administrationDisplays"/>-->
    <!--</g:HTMLPanel>-->
  </g:FlowPanel>
</ui:UiBinder>
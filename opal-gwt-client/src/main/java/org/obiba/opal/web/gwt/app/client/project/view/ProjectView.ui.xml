<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
    ui:generateLocales="default">

  <g:FlowPanel>
    <g:FlowPanel addStyleNames="page-header">
      <b:FluidRow>
        <b:Column size="9">
          <b:Breadcrumbs ui:field="breadcrumbs">
            <b:NavLink ui:field="projects">
              <ui:msg description="Projects label">Projects</ui:msg>
            </b:NavLink>
          </b:Breadcrumbs>
        </b:Column>
        <b:Column size="3">

        </b:Column>
      </b:FluidRow>
      <g:InlineLabel ui:field="description"></g:InlineLabel><b:Button type="LINK" icon="PLUS_SIGN" ui:field="ellipsis"/>
    </g:FlowPanel>
    <b:TabPanel tabPosition="right" ui:field="tabPanel">
      <b:Tab icon="TABLE" heading="Tables" active="true">
        <g:SimplePanel ui:field="tablesPanel"/>
      </b:Tab>
      <b:Tab icon="FILE" heading="Files">
        <b:Heading size="5">Files</b:Heading>
        <b:Paragraph>
          huhuhu hahha
        </b:Paragraph>
      </b:Tab>
      <b:Tab icon="BOOK" heading="Administration">
        <b:NavPills>
          <b:NavLink active="true">Settings</b:NavLink>
          <b:NavLink>Permissions</b:NavLink>
          <b:NavLink>Units</b:NavLink>
        </b:NavPills>
      </b:Tab>
    </b:TabPanel>
    <g:FlowPanel ui:field="content"></g:FlowPanel>
  </g:FlowPanel>

</ui:UiBinder>
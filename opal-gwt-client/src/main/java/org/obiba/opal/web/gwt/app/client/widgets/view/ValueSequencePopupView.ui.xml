<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui" 
    xmlns:g2="urn:import:com.google.gwt.user.cellview.client"
    xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view"  
    xmlns:pretty="urn:import:org.obiba.opal.web.gwt.prettify.client"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
    ui:generateLocales="default">

	<g:DialogBox ui:field="dialogBox" modal="false">
      <g:DockLayoutPanel styleName="main" ui:field="content" unit="EM" height="50em" width="50em">
        <g:center>
          <g:ScrollPanel>
            <g:FlowPanel addStyleNames="small-dual-indent top-margin">
              <o:PropertiesTable condensed="true" borderedCell="false" zebra="true" keyStyleNames="span2 small-indent" valueStyleNames="span8">
                <o:PropertyPanel>
                  <g:Label><ui:msg description="Entity type label">Entity Type</ui:msg></g:Label>
                  <g:Label ui:field="entityType"/>
                </o:PropertyPanel>
                <o:PropertyPanel column="1">
                  <g:Label><ui:msg description="Occurrence Group label">Occurrence Group</ui:msg></g:Label>
                  <g:Label ui:field="occurrenceGroup"/>
                </o:PropertyPanel>
              </o:PropertiesTable>
              <g:FlowPanel>
                <g:InlineLabel ui:field="noValues"><ui:msg description="No Variables label">No Values</ui:msg></g:InlineLabel>
                <o:Table width="100%" ui:field="valuesTable" addStyleNames="top-margin left-aligned"/>
              </g:FlowPanel>
            </g:FlowPanel>
          </g:ScrollPanel>
        </g:center>
        <g:south size="4">
          <g:FlowPanel addStyleNames="footer">
            <o:ResizeHandle ui:field="resizeHandle"></o:ResizeHandle>
            <g:Button styleName="btn right-aligned top-margin" ui:field="closeButton">
              <ui:msg description="Close label">Close</ui:msg>
            </g:Button>
          </g:FlowPanel>
        </g:south>
      </g:DockLayoutPanel>
    </g:DialogBox>
</ui:UiBinder>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
    ui:generateLocales="default">

  <b:TabPanel tabPosition="above">
    <b:Tab icon="LIST" active="true">
      <b:NavList>
        <b:NavHeader>Homes</b:NavHeader>
        <b:NavLink icon="USER">User</b:NavLink>
        <b:NavLink icon="BRIEFCASE">Project</b:NavLink>
        <b:NavHeader>System</b:NavHeader>
        <b:NavLink icon="HDD">File System</b:NavLink>
        <b:NavLink icon="EXCHANGE">Organizations</b:NavLink>
        <b:NavLink icon="FILE_ALT">Reports</b:NavLink>
        <b:NavHeader>Bookmarks</b:NavHeader>
      </b:NavList>
    </b:Tab>
    <b:Tab icon="SITEMAP">
      <g:Tree ui:field="fileSystemTree"></g:Tree>
    </b:Tab>
  </b:TabPanel>

</ui:UiBinder>
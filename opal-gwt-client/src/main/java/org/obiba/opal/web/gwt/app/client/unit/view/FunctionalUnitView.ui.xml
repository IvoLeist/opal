<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui" 
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

  
  <o:WorkbenchLayout withInfo="false">
    <g:Label><ui:msg description="Functional Units title">Units</ui:msg></g:Label>
    <g:Label></g:Label>
    <g:FlowPanel>
      <g:Button ui:field="functionalUnitButton">
        <ui:msg description="Add Functional Unit label">Add Unit</ui:msg>
      </g:Button>
      <g:Button ui:field="exportButton">
        <ui:msg description="Export All Identifiers label">Export All Identifiers</ui:msg>
      </g:Button>
      <g:Button ui:field="importButton">
        <ui:msg description="Import Identifiers label">Add/Update Identifiers</ui:msg>
      </g:Button>
    </g:FlowPanel>
    <g:SplitLayoutPanel>
      <g:center> 
        <g:ScrollPanel width="100%" ui:field="functionalUnitDetailsPanel" />
      </g:center>
      <g:west size='240'>
        <g:ScrollPanel ui:field="functionalUnitListPanel"/>
      </g:west>
    </g:SplitLayoutPanel>  
  </o:WorkbenchLayout>
  
</ui:UiBinder>
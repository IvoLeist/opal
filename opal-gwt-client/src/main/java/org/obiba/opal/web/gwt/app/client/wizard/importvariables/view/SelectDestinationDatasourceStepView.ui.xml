<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui" 
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

  <o:WorkbenchLayout>
    <g:Label><ui:msg description="Import Variables title">Import Variables</ui:msg></g:Label>
    <g:Label><ui:msg description="Select Destination Datasource summary">Select the datasource to be modified</ui:msg></g:Label>
    <g:FlowPanel>
      <g:Button ui:field="cancelButton"><ui:msg description="Cancel button label">Cancel</ui:msg></g:Button>    
      <g:Button ui:field="nextButton"><ui:msg description="Next button label">Next</ui:msg></g:Button>
    </g:FlowPanel>
    
    <g:FlowPanel>
      <g:HTMLPanel>
        <table class="gwt-KeyValue" cellspacing="10">
          <tbody>
            <tr>
              <td style="vertical-align:top">
                <ui:msg description="Datasource label">Datasource</ui:msg>:                
              </td>
              <td>
                <g:FormPanel ui:field="datasourceForm" method="POST">
                  <g:ListBox ui:field="datasource" name="datasource" />
                </g:FormPanel>
              </td>
            </tr>
          </tbody>
        </table>
      </g:HTMLPanel>
    </g:FlowPanel>
    
     <g:FlowPanel>
        <g:HTMLPanel>
          <g:Label addStyleNames="title"><ui:msg description="Datasource label">Datasource</ui:msg></g:Label>
          <p><ui:msg description="Datasource to modify information">Specify the datasource to modify.  Existing tables will be updated and new tables will be created.</ui:msg></p>
        </g:HTMLPanel>
     </g:FlowPanel>      
  </o:WorkbenchLayout>

</ui:UiBinder>
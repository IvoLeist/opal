<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module>

  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User' />
  <inherits name='com.google.gwt.xml.XML' />
  <inherits name='com.google.gwt.gen2.table.Table' />
  <inherits name='com.google.gwt.visualization.Visualization' />

  <inherits name='org.obiba.opal.web.model.Model' />

  <!-- Use an MVP pattern -->
  <inherits name='net.customware.gwt.presenter.Presenter' />
  <inherits name='com.google.gwt.inject.Inject' />

  <!--  inherit css based theme-->
  <inherits name='com.google.gwt.user.theme.standard.Standard' />

  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.obiba.opal.web.client.gwt.client.GwtApp' />

  <!-- Specify the application specific style sheet.             
      <stylesheet src='Application.css' /> 
      -->
  <set-property name="user.agent" value="gecko1_8" />
</module>

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui" 
  xmlns:c="urn:import:com.google.gwt.user.cellview.client" 
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

  <g:FlowPanel>
        
    <g:FlowPanel addStyleNames="panel">
      <g:Label addStyleNames="title"><ui:msg description="R server admin label" >R Server</ui:msg></g:Label>
      <g:Button ui:field="rTestButton" addStyleNames="content"><ui:msg description="R server test label" >Test connection with R</ui:msg></g:Button>
    </g:FlowPanel>
          
    <g:FlowPanel addStyleNames="panel">
      <g:Label addStyleNames="title"><ui:msg description="Aggregating methods label" >Aggregating Methods</ui:msg></g:Label>
      <g:VerticalPanel width="100%" addStyleNames="content">
        <g:InlineLabel ui:field="noMethods"><ui:msg description="No Aggregating methods label">No Aggregating Methods</ui:msg></g:InlineLabel>
        <c:SimplePager ui:field="methodsTablePager" location="CENTER" addStyleNames="right-aligned" />
        <o:Table width="100%" ui:field="methodsTable" addStyleNames="gwt-CellTable"/>
      </g:VerticalPanel>
    </g:FlowPanel>
          
  </g:FlowPanel>
</ui:UiBinder>
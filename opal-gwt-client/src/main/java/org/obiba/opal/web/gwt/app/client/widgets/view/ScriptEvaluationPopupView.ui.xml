<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:g2="urn:import:com.google.gwt.user.cellview.client"
	xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view"  
	xmlns:pretty="urn:import:org.obiba.opal.web.gwt.prettify.client"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default">

	<g:FlowPanel ui:field="content">
		<g:FlowPanel addStyleNames="footer">
			<g:Button styleName="btn right-aligned" ui:field="closeButton">
				<ui:msg description="Close label">Close</ui:msg>
			</g:Button>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>
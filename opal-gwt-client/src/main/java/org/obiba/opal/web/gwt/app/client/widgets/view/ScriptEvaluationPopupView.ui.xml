<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" 
	xmlns:g2="urn:import:com.google.gwt.user.cellview.client"
	xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view"  
	xmlns:pretty="urn:import:org.obiba.opal.web.gwt.prettify.client"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default">

	<g:FlowPanel ui:field="content">
		<g:DockLayoutPanel ui:field="dock" unit="EM">
			<g:east size="35">
				<pretty:PrettyPrintLabel ui:field="script"/>
			</g:east>
			<g:center>
				<g:FlowPanel>
					<g:FlowPanel addStyleNames="paging">
				        <g:Anchor ui:field="previousPage" addStyleNames="previous"></g:Anchor>
						<g:InlineLabel ui:field="pageLow">1</g:InlineLabel>
						<g:InlineLabel> ... </g:InlineLabel>
						<g:InlineLabel ui:field="pageHigh">10</g:InlineLabel>
						<g:Anchor ui:field="nextPage" addStyleNames="next"></g:Anchor>
			      	</g:FlowPanel>
					<o:Table width="100%" ui:field="valuesTable"/>
				</g:FlowPanel>
			</g:center>
			<g:south size="5">
				<g:FlowPanel addStyleNames="footer">
					<g:Button styleName="btn right-aligned" ui:field="closeButton">
						<ui:msg description="Close label">Close</ui:msg>
					</g:Button>
				</g:FlowPanel>
			</g:south>
		</g:DockLayoutPanel>
		
	</g:FlowPanel>
</ui:UiBinder>
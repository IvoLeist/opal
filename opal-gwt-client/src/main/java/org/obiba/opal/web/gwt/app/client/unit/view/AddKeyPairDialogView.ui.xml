<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  xmlns:g2="urn:import:com.google.gwt.user.cellview.client" 
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

  <ui:style>
    .optionsPanelMsg td { margin: 0.5em 0.5em 0.5em 0.5em; }
    .right-aligned { float: right; }
    .top-margin { margin-top: 0.5em; }
  </ui:style>

  <o:WizardDialogBox ui:field="dialog" autoHide="false" modal="true" height="10em" width="35em">
    <g:caption>
      <ui:msg description="Add Key Pair dialog title">Add Key Pair</ui:msg>
    </g:caption>
    <g:FlowPanel>
      <o:WizardStep ui:field="privateKeyStep">
      </o:WizardStep>
      <o:WizardStep ui:field="publicKeyCreateStep">
      </o:WizardStep>
      <o:WizardStep ui:field="publicKeyImportOrCreateStep">
      </o:WizardStep>
    </g:FlowPanel>
    
  </o:WizardDialogBox>

</ui:UiBinder>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:g2="urn:import:com.google.gwt.user.cellview.client" 
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

  <o:WorkbenchLayout>
    <g:Label><ui:msg description="Explorer">File Explorer</ui:msg></g:Label>
    <g:Label></g:Label>
    <g:FlowPanel>
      <g:Button ui:field="createFolderButton" styleName="btn icon-before i-plus">
        <ui:msg description="Create Folder label">Create Folder</ui:msg>
      </g:Button>          
      <g:Button ui:field="fileUploadButton" styleName="btn icon-before i-up">
        <ui:msg description="Upload File label">Upload</ui:msg>
      </g:Button>
      <g:Button ui:field="fileDownloadButton" styleName="btn icon-before i-down">
        <ui:msg description="Download File label">Download</ui:msg>
      </g:Button>
      <g:Button ui:field="fileDeleteButton" styleName="btn icon-before i-disapprove">
        <ui:msg description="Delete File label">Delete</ui:msg>
      </g:Button>
      <o:HelpImage page="Working+with+Files"></o:HelpImage>
    </g:FlowPanel>
    <g:SplitLayoutPanel>
        <g:west size="200">
          <g:ScrollPanel ui:field="fileSystemTreePanel">
          </g:ScrollPanel>
        </g:west>
        <g:center>
          <g:ScrollPanel ui:field="folderDetailsPanel">
          </g:ScrollPanel>
        </g:center>
      </g:SplitLayoutPanel>
  </o:WorkbenchLayout>

</ui:UiBinder>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:g2="urn:import:com.google.gwt.user.cellview.client"
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

  <ui:style>
    .list-header {
      margin-left: 1em;
      margin-top: 1em;
    }
    
    .return-label {
      margin-top: 1em;
      margin-left: 1em;
    }
    
    .return-button {
      margin-left: 2em;
    }
  </ui:style>
  
  <o:WorkbenchLayout withInfo="false">
    <g:Label><ui:msg description="Import Variables title">Import Variables</ui:msg></g:Label>
    <g:Label><ui:msg description="Import Variables summary">Importing variables</ui:msg></g:Label>
    <g:SimplePanel />
    
    <g:FlowPanel>
      <g:Label addStyleNames="{style.list-header}"><ui:msg description="Created/updated table list header">Importing variables to the following tables:</ui:msg></g:Label>
      <g:FlowPanel ui:field="tableListPanel" />
      <g:Label addStyleNames="{style.return-label}"><ui:msg description="Return to Import Variables label">You may now import a new variables dictionary:</ui:msg></g:Label>
      <g:Button ui:field="returnButton" addStyleNames="{style.return-label}"><ui:msg description="Return to Import Variables button label">Return to Import Variables</ui:msg></g:Button>
    </g:FlowPanel> 
  </o:WorkbenchLayout>

</ui:UiBinder>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:g2="urn:import:com.google.gwt.user.cellview.client" 
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

  <g:FlowPanel ui:field="functionalUnitDetails" addStyleNames="item-details">

    <g:FlowPanel ui:field="toolbarPanel" addStyleNames="toolbar toolbar-left"/>

    <g:HTMLPanel>
      <table class="item-properties small-indent">
        <tr>
          <td>
            <g:Label addStyleNames="key">
              <ui:msg description="Identifier Variables Filter label">Identifier Variables Filter</ui:msg>:
            </g:Label>
          </td>
          <td>
            <g:Label ui:field="select"/>
          </td>
        </tr>
      </table>
    </g:HTMLPanel>

    <g:HTMLPanel>
      <table class="item-properties small-indent">
        <tr>
          <td>
            <g:Label addStyleNames="key">
              <ui:msg description="Current Count of Identifiers label">Current Count of Identifiers</ui:msg>:
            </g:Label>
          </td>
          <td>
            <g:Label ui:field="currentCountOfIdentifiers"/>
          </td>
        </tr>
      </table>
    </g:HTMLPanel>

    <o:HorizontalTabLayout ui:field="tabs" addStyleNames="small-indent top-margin not-so-full-width">
	  <g:Anchor><ui:msg description="Keystore title">Keystore</ui:msg></g:Anchor>
      <g:VerticalPanel width="100%" horizontalAlignment="ALIGN_RIGHT">
        <g:InlineLabel ui:field="noKeyPairs"><ui:msg description="No Key Pairs label">No Key Pairs</ui:msg></g:InlineLabel>
        <g2:CellTable ui:field="keyPairsTable" width="100%" addStyleNames="left-aligned"/>
      </g:VerticalPanel>
    </o:HorizontalTabLayout>

  </g:FlowPanel>

</ui:UiBinder>
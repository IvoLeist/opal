<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:g2="urn:import:com.google.gwt.user.cellview.client"
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

  <o:WorkbenchLayout withInfo="false">
    <g:Label><ui:msg description="Data import title">Import Data</ui:msg></g:Label>
    <g:Label><ui:msg description="Validation Report Step summary">Some formatting errors have been detected. You should fix them before importing variables.</ui:msg></g:Label>
    <g:Button ui:field="cancelButton"><ui:msg description="Cancel button label">Cancel</ui:msg></g:Button>
    
    <g:FlowPanel>
      <g2:CellTable width="100%" ui:field="parsingTable" />
      <g2:CellTable width="100%" ui:field="validationTable" />
    </g:FlowPanel> 
  </o:WorkbenchLayout>

</ui:UiBinder>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:g2="urn:import:com.google.gwt.user.cellview.client" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" ui:generateLocales="default">

  <g:FlowPanel ui:field="reportTemplateDetails" addStyleNames="item-details">

    <g:FlowPanel ui:field="toolbarPanel" addStyleNames="toolbar toolbar-left"/>

    <g:HTMLPanel>
      <table class="item-properties small-indent">
        <tr>
          <td>
            <g:Label addStyleNames="key">
              <ui:msg description="BIRT Report Design label">BIRT Report Design:</ui:msg>
            </g:Label>
          </td>
          <td>
            <g:Anchor ui:field="design"/>
          </td>
        </tr>
        <tr>
          <td>
            <g:Label addStyleNames="key">
              <ui:msg description="Schedule label">Schedule:</ui:msg>
            </g:Label>
          </td>
          <td>
            <g:Label ui:field="schedule"/>
          </td>
        </tr>
        <tr>
          <td>
            <g:Label addStyleNames="key">
              <ui:msg description="Format label">Format:</ui:msg>
            </g:Label>
          </td>
          <td>
            <g:Label ui:field="format"/>
          </td>
        </tr>
        <tr>
          <td>
            <g:Label addStyleNames="key">
              <ui:msg description="Parameters label">Parameters:</ui:msg>
            </g:Label>
          </td>
          <td>
            <g:Label ui:field="parameters"/>
          </td>
        </tr>
        <tr>
          <td>
            <g:Label addStyleNames="key">
              <ui:msg description="Notification emails label">Notifications Emails:</ui:msg>
            </g:Label>
          </td>
          <td>
            <g:Label ui:field="emails"/>
          </td>
        </tr>
      </table>
    </g:HTMLPanel>

    <g:FlowPanel ui:field="producedReports" addStyleNames="producedReports">
      <g:Label addStyleNames="title">
        <ui:msg description="Reports title">Reports</ui:msg>
      </g:Label>
      <g:VerticalPanel width="100%">
        <g2:CellTable ui:field="producedReportsTable" width="100%"/>
      </g:VerticalPanel>
    </g:FlowPanel>
    
  </g:FlowPanel>

</ui:UiBinder>
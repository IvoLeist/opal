<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:g2="urn:import:com.google.gwt.user.cellview.client"
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
  ui:generateLocales="default">

  <g:FlowPanel>
    <g:Label addStyleNames="title">
      <ui:msg description="Reports title">Reports</ui:msg>
    </g:Label>
    <g:VerticalPanel width="100%">
      <g2:CellTable ui:field="producedReportsTable"
        addStyleNames="producedReports" width="100%"/>
    </g:VerticalPanel>
  </g:FlowPanel>

</ui:UiBinder>
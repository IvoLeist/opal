<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui" 
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  xmlns:w="urn:import:org.obiba.opal.web.gwt.app.client.wizard.importdata.view"  
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

  <g:FlowPanel>
    <g:FlowPanel styleName="input">
      <g:Label addStyleNames="key"><ui:msg description="Table label">Table</ui:msg></g:Label>
      <g:ListBox ui:field="tableList"/>
    </g:FlowPanel>
    <g:SimplePanel ui:field="tableValuesPanel"/>
  </g:FlowPanel>    
  
</ui:UiBinder>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
    xmlns:g="urn:import:com.google.gwt.user.client.ui"
    xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
    xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.ui"
    ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
    ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
    ui:generateLocales="default">

  <g:FlowPanel>
    <g:FlowPanel addStyleNames="input">
      <g:InlineLabel ui:field="typeLabel" addStyleNames="key"/>
      <g:SuggestBox ui:field="principal"/>
      <g:Button ui:field="add" styleName="btn btn-primary xsmall-indent">
        <ui:msg description="Add button label">Add</ui:msg>
      </g:Button>
    </g:FlowPanel>
    <g:FlowPanel addStyleNames="top-margin">
      <b:SimplePager ui:field="pager" location="RIGHT"/>
      <o:Table ui:field="table" addStyleNames="full-width"/>
    </g:FlowPanel>
  </g:FlowPanel>
</ui:UiBinder>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:g2="urn:import:com.google.gwt.user.cellview.client" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" ui:generateLocales="default">

  <g:DockLayoutPanel ui:field="workbench" addStyleNames="workbench">
    <g:north size="35">
      <g:FlowPanel addStyleNames="top">
        <g:FlowPanel ui:field="topHeader" addStyleNames="header"/>
        <g:SimplePanel ui:field="controlContent" addStyleNames="control" />
      </g:FlowPanel>
    </g:north>
    <g:center>
      <g:SplitLayoutPanel ui:field="content">
        <g:center>  
          <g:ScrollPanel ui:field="mainContent" addStyleNames="main" />
        </g:center>
      </g:SplitLayoutPanel>
    </g:center>
  </g:DockLayoutPanel>

</ui:UiBinder>

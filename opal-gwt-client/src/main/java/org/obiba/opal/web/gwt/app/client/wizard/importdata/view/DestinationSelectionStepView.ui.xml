<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui" 
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">

<!--    <g:Label><ui:msg description="Destination selection summary">Select the destination (where you wish to write the data to)</ui:msg></g:Label>-->
    <g:HTMLPanel ui:field="step">
        <table class="gwt-KeyValue" cellspacing="10">
          <tbody>
            <tr>
              <td>
                <ui:msg description="Destination dataset label">Destination Dataset:</ui:msg>        
              </td>
              <td>
                <g:ListBox ui:field='datasetListBox' visibleItemCount='1'/>
              </td>
            </tr>
            <tr>
              <td>
                <div ui:field='tableListBoxLabel'><ui:msg description="Destination table label">Destination Table:</ui:msg></div>  
              </td>
              <td>
                <g:ListBox ui:field='tableListBox' visibleItemCount='1'/>
              </td>
            </tr>
          </tbody>
        </table>
    </g:HTMLPanel>   
    
  
</ui:UiBinder>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" 
  xmlns:g="urn:import:com.google.gwt.user.client.ui"
  xmlns:c="urn:import:com.google.gwt.user.cellview.client"
  xmlns:o="urn:import:org.obiba.opal.web.gwt.app.client.workbench.view" 
  ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator" 
  ui:generateLocales="default">
  
  <g:FlowPanel ui:field="valuesPanel">
     <g:InlineLabel ui:field="noValues"><ui:msg description="No Variables label">No Values</ui:msg></g:InlineLabel>
     <g:FlowPanel addStyleNames="paging right-aligned">
        <g:Anchor ui:field="previousPage" addStyleNames="icon icon-previous"></g:Anchor>
		<g:InlineLabel ui:field="pageLow">1</g:InlineLabel>
		<g:InlineLabel> ... </g:InlineLabel>
		<g:InlineLabel ui:field="pageHigh">10</g:InlineLabel>
		<g:Anchor ui:field="nextPage" addStyleNames="icon icon-next"></g:Anchor>
      </g:FlowPanel>
     <o:Table ui:field="valuesTable" addStyleNames="full-width left-aligned"></o:Table>
  </g:FlowPanel>
  
</ui:UiBinder>